#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sergio/Escritorio/recommended-books-page/node_modules/.pnpm/astro@5.16.14_@vercel+functions@2.2.13_jiti@2.6.1_lightningcss@1.30.2_rollup@4.56.0_typescript@5.9.3_yaml@2.8.2/node_modules/astro/node_modules:/home/sergio/Escritorio/recommended-books-page/node_modules/.pnpm/astro@5.16.14_@vercel+functions@2.2.13_jiti@2.6.1_lightningcss@1.30.2_rollup@4.56.0_typescript@5.9.3_yaml@2.8.2/node_modules:/home/sergio/Escritorio/recommended-books-page/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sergio/Escritorio/recommended-books-page/node_modules/.pnpm/astro@5.16.14_@vercel+functions@2.2.13_jiti@2.6.1_lightningcss@1.30.2_rollup@4.56.0_typescript@5.9.3_yaml@2.8.2/node_modules/astro/node_modules:/home/sergio/Escritorio/recommended-books-page/node_modules/.pnpm/astro@5.16.14_@vercel+functions@2.2.13_jiti@2.6.1_lightningcss@1.30.2_rollup@4.56.0_typescript@5.9.3_yaml@2.8.2/node_modules:/home/sergio/Escritorio/recommended-books-page/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
